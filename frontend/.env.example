# WebSocket URL for connecting to the Java WebSocket server
# Browser connects to exposed port on localhost
VITE_WS_URL=ws://localhost:8080/ws/chat

# API URL for connecting to the Python AI service
# Browser calls directly to exposed port (not via proxy)
# This must be http://localhost:8000 because browser cannot access Docker network names
VITE_API_URL=http://localhost:8000
